// Componente para input de par√°metros
interface ParameterInputProps {
  parameter: ParameterInfo;
  value: string | number | boolean;
  onChange: (value: any) => void;
}

function ParameterInput({ parameter, value, onChange }: ParameterInputProps) {
  const renderInput = () => {
    switch (parameter.type) {
      case 'boolean':
        return (
          <FormControlLabel
            control={
              <Switch
                checked={Boolean(value)}
                onChange={(e) => onChange(e.target.checked)}
                sx={{
                  '& .MuiSwitch-switchBase.Mui-checked': {
                    color: '#ffb74d',
                    '&:hover': {
                      backgroundColor: 'rgba(255, 183, 77, 0.08)',
                    },
                  },
                  '& .MuiSwitch-switchBase.Mui-checked + .MuiSwitch-track': {
                    backgroundColor: '#ffb74d',
                  },
                }}
              />
            }
            label=""
            sx={{ margin: 0 }}
          />
        );

      case 'select':
        return (
          <FormControl fullWidth size="small">
            <Select
              value={value}
              onChange={(e) => onChange(e.target.value)}
              sx={{
                color: '#fff',
                '& .MuiOutlinedInput-notchedOutline': { borderColor: '#666' },
                '&:hover .MuiOutlinedInput-notchedOutline': { borderColor: '#ffb74d' },
                '&.Mui-focused .MuiOutlinedInput-notchedOutline': { borderColor: '#ffb74d' },
                '& .MuiSvgIcon-root': { color: '#fff' },
              }}
            >
              {parameter.options?.map((option) => (
                <MenuItem key={option} value={option}>
                  {option}
                </MenuItem>
              ))}
            </Select>
          </FormControl>
        );

      case 'number':
      case 'integer':
        if (parameter.range) {
          const [min, max] = parameter.range;
          return (
            <Box>
              <Slider
                value={Number(value)}
                min={min}
                max={max}
                step={parameter.type === 'integer' ? 1 : 0.1}
                onChange={(_, newValue) => onChange(newValue)}
                valueLabelDisplay="auto"
                sx={{
                  color: '#ffb74d',
                  '& .MuiSlider-thumb': {
                    backgroundColor: '#ffb74d',
                  },
                  '& .MuiSlider-track': {
                    backgroundColor: '#ffb74d',
                  },
                  '& .MuiSlider-rail': {
                    backgroundColor: '#666',
                  },
                  marginBottom: 1,
                }}
              />
              <TextField
                fullWidth
                size="small"
                type="number"
                value={value}
                onChange={(e) => {
                  const newValue = parameter.type === 'integer' 
                    ? parseInt(e.target.value) 
                    : parseFloat(e.target.value);
                  if (!isNaN(newValue)) onChange(newValue);
                }}
                InputProps={{
                  endAdornment: parameter.unit ? (
                    <Typography variant="body2" sx={{ color: '#b0b0b0', marginLeft: 1 }}>
                      {parameter.unit}
                    </Typography>
                  ) : null,
                }}
                sx={{
                  '& .MuiOutlinedInput-root': {
                    color: '#fff',
                    '& fieldset': { borderColor: '#666' },
                    '&:hover fieldset': { borderColor: '#ffb74d' },
                    '&.Mui-focused fieldset': { borderColor: '#ffb74d' },
                  },
                  '& input[type=number]': {
                    '&::-webkit-outer-spin-button, &::-webkit-inner-spin-button': {
                      '-webkit-appearance': 'none',
                      margin: 0,
                    },
                    '-moz-appearance': 'textfield',
                  },
                }}
              />
            </Box>
          );
        } else {
          return (
            <TextField
              fullWidth
              size="small"
              type="number"
              value={value}
              onChange={(e) => {
                const newValue = parameter.type === 'integer' 
                  ? parseInt(e.target.value) 
                  : parseFloat(e.target.value);
                if (!isNaN(newValue)) onChange(newValue);
              }}
              InputProps={{
                endAdornment: parameter.unit ? (
                  <Typography variant="body2" sx={{ color: '#b0b0b0', marginLeft: 1 }}>
                    {parameter.unit}
                  </Typography>
                ) : null,
              }}
              sx={{
                '& .MuiOutlinedInput-root': {
                  color: '#fff',
                  '& fieldset': { borderColor: '#666' },
                  '&:hover fieldset': { borderColor: '#ffb74d' },
                  '&.Mui-focused fieldset': { borderColor: '#ffb74d' },
                },
                '& input[type=number]': {
                  '&::-webkit-outer-spin-button, &::-webkit-inner-spin-button': {
                    '-webkit-appearance': 'none',
                    margin: 0,
                  },
                  '-moz-appearance': 'textfield',
                },
              }}
            />
          );
        }

      default:
        return (
          <TextField
            fullWidth
            size="small"
            value={value}
            onChange={(e) => onChange(e.target.value)}
            sx={{
              '& .MuiOutlinedInput-root': {
                color: '#fff',
                '& fieldset': { borderColor: '#666' },
                '&:hover fieldset': { borderColor: '#ffb74d' },
                '&.Mui-focused fieldset': { borderColor: '#ffb74d' },
              },
            }}
          />
        );
    }
  };

  return (
    <Box>
      <Box sx={{ display: 'flex', alignItems: 'center', marginBottom: 1 }}>
        <Typography variant="body2" sx={{ color: '#fff', fontWeight: 'bold' }}>
          {parameter.label}
        </Typography>
        {parameter.description && (
          <Tooltip title={parameter.description} arrow>
            <InfoIcon sx={{ color: '#b0b0b0', fontSize: 16, marginLeft: 0.5 }} />
          </Tooltip>
        )}
      </Box>
      {renderInput()}
      {parameter.range && (
        <Typography variant="caption" sx={{ color: '#888', display: 'block', marginTop: 0.5 }}>
          Rango: {parameter.range[0]} - {parameter.range[1]} {parameter.unit}
        </Typography>
      )}
    </Box>
  );
}

// Mapear circuit_type a key del estado
const getCalculationKey = (circuitType: string): keyof CalculationState => {
  switch (circuitType) {
    case "dc_strings": return "strings";
    case "level_1_dc": return "level1";
    case "ac_circuits": return "ac";
    case "mv_circuits": return "mv";
    default: return "strings";
  }
};

