import { useState, useEffect } from "react";
import { useProject } from "../context/ProjectContext";
import { useParams, useNavigate } from "react-router-dom";
import axios from "axios";
import {
  Box,
  Typography,
  Button,
  Paper,
  Tabs,
  Tab,
  FormControl,
  InputLabel,
  Select,
  MenuItem,
  Alert,
  CircularProgress,
  Chip,
  LinearProgress,
  Grid,
  Collapse,
  IconButton,
  TextField,
  Accordion,
  AccordionSummary,
  AccordionDetails,
  Switch,
  FormControlLabel,
  Slider,
  Tooltip,
} from "@mui/material";
import { DataGrid } from "@mui/x-data-grid";
import type { GridColDef } from "@mui/x-data-grid";
import ArrowBackIcon from '@mui/icons-material/ArrowBack';
import CalculateIcon from '@mui/icons-material/Calculate';
import AssessmentIcon from '@mui/icons-material/Assessment';
import ElectricalServicesIcon from '@mui/icons-material/ElectricalServices';
import PowerIcon from '@mui/icons-material/Power';
import FlashOnIcon from '@mui/icons-material/FlashOn';
import SummarizeIcon from '@mui/icons-material/Summarize';
import ExpandMoreIcon from '@mui/icons-material/ExpandMore';
import ExpandLessIcon from '@mui/icons-material/ExpandLess';
import RefreshIcon from '@mui/icons-material/Refresh';
import SettingsIcon from '@mui/icons-material/Settings';
import SaveIcon from '@mui/icons-material/Save';
import RestoreIcon from '@mui/icons-material/Restore';
import InfoIcon from '@mui/icons-material/Info';

// Configuraci√≥n API
const API_BASE_URL = import.meta.env.VITE_API_URL || "http://localhost:8000";

// Interfaces para tipos de datos
interface CalculationResult {
  results: any[];
  summary?: any;
  timestamp: string;
  status: 'success' | 'error' | 'pending';
  error?: string;
}

interface CalculationState {
  strings: CalculationResult | null;
  level1: CalculationResult | null;
  ac: CalculationResult | null;
  mv: CalculationResult | null;
}

interface ProjectProgress {
  completed: number;
  total: number;
  percentage: number;
}

interface ParameterInfo {
  value: string | number | boolean;
  label: string;
  description: string;
  type: 'number' | 'integer' | 'select' | 'boolean';
  unit?: string;
  range?: [number, number];
  options?: string[];
  depends_on?: string;
  visible_when?: string;
}

interface ParameterSection {
  title: string;
  parameters: Record<string, ParameterInfo>;
}

interface NormativeConfig {
  norm_name: string;
  display_name: string;
  description: string;
  country: string;
  editable_sections: Record<string, ParameterSection>;
  has_project_overrides: boolean;
  project_name?: string;
}

// Componente de pesta√±a personalizada
interface TabPanelProps {
  children?: React.ReactNode;
  index: number;
  value: number;
}

function TabPanel({ children, value, index }: TabPanelProps) {
  return (
    <div
      role="tabpanel"
      hidden={value !== index}
      id={`calculation-tabpanel-${index}`}
      aria-labelledby={`calculation-tab-${index}`}
    >
      {value === index && <Box sx={{ p: 3 }}>{children}</Box>}
    </div>
  );
}

// Componente principal
function StringCalculationPage() {
  const { projectName, setProjectName } = useProject();
  const { projectName: urlProjectName } = useParams<{ projectName: string }>();
  const navigate = useNavigate();
  
  // Estados principales
  const [activeTab, setActiveTab] = useState(0);
  const [norm, setNorm] = useState<"IEC" | "NEC" | "Personalizada">("IEC");
  const [calculations, setCalculations] = useState<CalculationState>({
    strings: null,
    level1: null,
    ac: null,
    mv: null,
  });
  const [isLoading, setIsLoading] = useState(false);
  const [error, setError] = useState("");
  const [success, setSuccess] = useState("");
  const [isAnalysisExpanded, setIsAnalysisExpanded] = useState(false);

  // Estados para configuraci√≥n de normativas
  const [normativeConfig, setNormativeConfig] = useState<NormativeConfig | null>(null);
  const [normativeLoading, setNormativeLoading] = useState(false);
  const [hasUnsavedChanges, setHasUnsavedChanges] = useState(false);

  // Establecer el proyecto desde la URL
  useEffect(() => {
    if (!projectName && urlProjectName) {
      setProjectName(urlProjectName);
    }
  }, [projectName, urlProjectName, setProjectName]);

  const currentProjectName = projectName || urlProjectName;

  // Cargar configuraci√≥n de normativa cuando cambie el proyecto o normativa
  useEffect(() => {
    if (currentProjectName && activeTab === 4) { // Tab de configuraci√≥n
      loadNormativeConfig();
    }
  }, [currentProjectName, norm, activeTab]);

  // Funci√≥n para cargar configuraci√≥n de normativa
  const loadNormativeConfig = async () => {
    if (!currentProjectName) return;

    setNormativeLoading(true);
    try {
      const response = await axios.get(
        `${API_BASE_URL}/calculations/projects/${currentProjectName}/normative-parameters?normative=${norm}`
      );
      setNormativeConfig(response.data);
    } catch (error: any) {
      console.error('Error loading normative config:', error);
      setError(`Error cargando configuraci√≥n: ${error.response?.data?.detail || error.message}`);
    } finally {
      setNormativeLoading(false);
    }
  };

  // Funci√≥n para guardar configuraci√≥n de normativa
  const saveNormativeConfig = async () => {
    if (!currentProjectName || !normativeConfig) return;

    setNormativeLoading(true);
    try {
      // Funci√≥n para construir estructura YAML
      const buildYamlStructure = () => {
        const yamlStructure: Record<string, any> = {};
        
        Object.entries(normativeConfig.editable_sections).forEach(([sectionKey, section]) => {
          Object.entries(section.parameters).forEach(([paramKey, param]) => {
            
            // Mapear directamente a estructura YAML
            switch (sectionKey) {
              case 'basic':
                if (!yamlStructure.correction_factors) yamlStructure.correction_factors = {};
                if (paramKey === 'isc_safety_factor') {
                  yamlStructure.correction_factors.isc_safety_factor = param.value;
                } else if (paramKey === 'parallel_strings') {
                  yamlStructure.correction_factors.parallel_strings = param.value;
                }
                break;
                
              case 'cable':
                if (!yamlStructure.cable) yamlStructure.cable = {};
                yamlStructure.cable[paramKey] = param.value;
                break;
                
              case 'installation':
                if (!yamlStructure.installation) yamlStructure.installation = {};
                yamlStructure.installation[paramKey] = param.value;
                break;
                
              case 'temperature':
                if (!yamlStructure.temperature_correction) yamlStructure.temperature_correction = {};
                if (paramKey === 'ambient_design') {
                  yamlStructure.temperature_correction.ambient_design = param.value;
                }
                break;
                
              case 'voltage':
                if (!yamlStructure.voltage_drop) yamlStructure.voltage_drop = {};
                yamlStructure.voltage_drop[paramKey] = param.value;
                break;
                
              case 'safety':
                if (!yamlStructure.safety_factors) yamlStructure.safety_factors = {};
                yamlStructure.safety_factors[paramKey] = param.value;
                break;
            }
          });
        });
        
        return yamlStructure;
      };

      // Construir estructura YAML
      const yamlStructure = buildYamlStructure();

      // ‚úÖ Validar que no est√© vac√≠a
      if (!yamlStructure || Object.keys(yamlStructure).length === 0) {
        setError('No hay cambios que guardar');
        setNormativeLoading(false);
        return;
      }

      console.log("üî• Enviando estructura YAML:", yamlStructure);
      console.log("üî• Base norm:", norm);
      console.log("üî• Project name:", currentProjectName);
      
      // Enviar al backend
      await axios.put(
        `${API_BASE_URL}/calculations/projects/${currentProjectName}/normative-parameters`,
        {
          base_norm: norm,
          yaml_overrides: yamlStructure  // ‚úÖ Estructura YAML directa
        }
      );

      setSuccess('Configuraci√≥n personalizada guardada exitosamente');
      setHasUnsavedChanges(false);

      // ‚úÖ NO resetear la UI - mantener valores actuales
      if (normativeConfig) {
        setNormativeConfig({
          ...normativeConfig,
          has_project_overrides: true
        });
      }

      // ‚úÖ Verificar que se use en c√°lculos (opcional)
      setTimeout(() => {
        verifyCalculationUsesProjectNormative();
      }, 1000);
      
    } catch (error: any) {
      console.error('Error saving normative config:', error);
      console.error('Error details:', error.response?.data);
      
      let errorMessage = 'Error desconocido';
      if (error.response?.data?.detail) {
        errorMessage = error.response.data.detail;
      } else if (error.message) {
        errorMessage = error.message;
      }
      
      setError(`Error guardando configuraci√≥n: ${errorMessage}`);
    } finally {
      setNormativeLoading(false);
    }
  };
  // Funci√≥n para resetear configuraci√≥n
  const resetNormativeConfig = async () => {
    if (!currentProjectName) return;

    setNormativeLoading(true);
    try {
      await axios.delete(
        `${API_BASE_URL}/calculations/projects/${currentProjectName}/normative-parameters`
      );

      setSuccess('Configuraci√≥n reseteada a valores por defecto');
      setHasUnsavedChanges(false);
      
      // Recargar configuraci√≥n
      loadNormativeConfig();
      
    } catch (error: any) {
      console.error('Error resetting normative config:', error);
      setError(`Error reseteando configuraci√≥n: ${error.response?.data?.detail || error.message}`);
    } finally {
      setNormativeLoading(false);
    }
  };

  // Calcular progreso - solo cuenta strings por ahora
  const getProgress = (): ProjectProgress => {
    const completed = calculations.strings?.status === 'success' ? 1 : 0;
    const total = 1; // Solo strings por ahora
    const percentage = total > 0 ? (completed / total) * 100 : 0;
    
    return { completed, total, percentage };
  };

  const progress = getProgress();

  // Configuraci√≥n de pesta√±as - agregar configuraci√≥n
  const tabs = [
    {
      label: "Strings DC",
      icon: <ElectricalServicesIcon />,
      description: "C√°lculo de strings fotovoltaicos",
      circuitType: "dc_strings",
      enabled: true
    },
    {
      label: "Nivel 1 DC",
      icon: <PowerIcon />,
      description: "Combinadores y concentradores DC",
      circuitType: "level_1_dc", 
      enabled: false
    },
    {
      label: "Circuitos AC",
      icon: <ElectricalServicesIcon />,
      description: "Circuitos de corriente alterna",
      circuitType: "ac_circuits",
      enabled: false
    },
    {
      label: "Media Tensi√≥n",
      icon: <FlashOnIcon />,
      description: "Circuitos de media tensi√≥n",
      circuitType: "mv_circuits",
      enabled: false
    },
    {
      label: "Configuraci√≥n",
      icon: <SettingsIcon />,
      description: "Par√°metros de normativa personalizables",
      circuitType: "config",
      enabled: true
    },
    {
      label: "Resumen",
      icon: <SummarizeIcon />,
      description: "Resumen ejecutivo y reportes",
      circuitType: "summary",
      enabled: true
    }
  ];

  // Funci√≥n para ejecutar c√°lculo de strings
  const executeStringCalculation = async () => {
    if (!currentProjectName) {
      setError("No hay proyecto seleccionado");
      return;
    }

    setIsLoading(true);
    setError("");
    setSuccess("");

    try {
      const response = await axios.get(
        `${API_BASE_URL}/calculations/calculate-strings/${currentProjectName}?normative=${norm}`
      );

      console.log('String calculation response:', response.data);
      
      const calculationResult: CalculationResult = {
        results: response.data.results || response.data,
        summary: response.data.summary,
        timestamp: new Date().toISOString(),
        status: 'success'
      };

      setCalculations(prev => ({
        ...prev,
        strings: calculationResult
      }));

      setSuccess(`C√°lculo de strings completado exitosamente. ${calculationResult.results.length} strings calculados.`);

    } catch (error: any) {
      console.error('Error calculando strings:', error);
      
      const calculationResult: CalculationResult = {
        results: [],
        timestamp: new Date().toISOString(),
        status: 'error',
        error: error.response?.data?.detail || error.message
      };

      setCalculations(prev => ({
        ...prev,
        strings: calculationResult
      }));

      setError(`Error en c√°lculo de strings: ${error.response?.data?.detail || error.message}`);
    } finally {
      setIsLoading(false);
    }
  };

  // Funci√≥n para limpiar resultados
  const clearResults = () => {
    setCalculations({
      strings: null,
      level1: null,
      ac: null,
      mv: null,
    });
    setError("");
    setSuccess("");
  };

  // Funci√≥n para volver
  const handleGoBack = () => {
    if (currentProjectName) {
      navigate(`/projects/${currentProjectName}/upload`);
    } else {
      navigate('/projects');
    }
  };

  // Funci√≥n para manejar cambios en normativa
  const handleNormChange = (newNorm: "IEC" | "NEC" | "Personalizada") => {
    setNorm(newNorm);
    // Limpiar configuraci√≥n cargada para forzar recarga
    setNormativeConfig(null);
    setHasUnsavedChanges(false);
  };

  // Mostrar error si no hay proyecto
  if (!currentProjectName) {
    return (
      <Box sx={{ 
        minHeight: '100vh',
        background: 'linear-gradient(135deg, #2c2c2c 0%, #3a3a3a 50%, #424242 100%)',
        padding: 3,
        display: 'flex',
        alignItems: 'center',
        justifyContent: 'center'
      }}>
        <Paper elevation={6} sx={{ 
          padding: 4, 
          textAlign: 'center',
          backgroundColor: '#3a3a3a',
          borderRadius: '16px',
          border: '1px solid #525252',
        }}>
          <Typography variant="h5" sx={{ color: '#ffa726', marginBottom: 2, fontSize: '48px' }}>
            !
          </Typography>
          <Typography variant="h6" sx={{ color: '#fff', marginBottom: 2 }}>
            No hay proyecto seleccionado
          </Typography>
          <Button
            variant="contained"
            onClick={() => navigate('/projects')}
            sx={{ 
              backgroundColor: '#666',
              color: 'white',
              fontWeight: 'bold',
              borderRadius: '8px',
              '&:hover': {
                backgroundColor: '#555',
              },
            }}
          >
            Seleccionar Proyecto
          </Button>
        </Paper>
      </Box>
    );
  }

  // ‚úÖ NUEVA: Funci√≥n para verificar que se us√≥ la normativa correcta
  const verifyCalculationUsesProjectNormative = async () => {
    if (!currentProjectName) return;
    
    try {
      console.log("üîç Verificando si los c√°lculos usan normativa del proyecto...");
      
      // Hacer un c√°lculo de prueba
      const response = await axios.get(
        `${API_BASE_URL}/calculations/calculate-strings/${currentProjectName}?normative=PERSONALIZADA`
      );
      
      console.log("‚úÖ C√°lculo realizado, verificar logs del backend para confirmar uso de normativa del proyecto");
      
    } catch (error) {
      console.error("Error en verificaci√≥n:", error);
    }
  };

  return (
    <Box sx={{ 
      minHeight: '100vh',
      background: 'linear-gradient(135deg, #2c2c2c 0%, #3a3a3a 50%, #424242 100%)',
      padding: 3, 
    }}>
      {/* Header del proyecto */}
      <Paper elevation={6} sx={{ 
        padding: 3, 
        marginBottom: 3,
        backgroundColor: '#3a3a3a',
        borderRadius: '16px',
        border: '1px solid #525252',
      }}>
        <Box sx={{ display: 'flex', alignItems: 'center', justifyContent: 'space-between', flexWrap: 'wrap', gap: 2 }}>
          <Box sx={{ display: 'flex', alignItems: 'center', gap: 2 }}>
            <Typography variant="h4" sx={{ 
              color: '#fff', 
              fontWeight: 'bold',
              display: 'flex',
              alignItems: 'center',
              gap: 2
            }}>
              C√°lculos El√©ctricos
            </Typography>
            <Chip 
              label={currentProjectName}
              sx={{ 
                backgroundColor: '#525252',
                color: '#e0e0e0',
                fontWeight: 'bold',
                fontSize: '14px'
              }}
            />
            {normativeConfig?.has_project_overrides && (
              <Chip 
                label="Configuraci√≥n personalizada"
                sx={{ 
                  backgroundColor: '#4a4a3a',
                  color: '#ffcc80',
                  fontWeight: 'bold',
                  fontSize: '12px'
                }}
              />
            )}
          </Box>

          <Box sx={{ display: 'flex', alignItems: 'center', gap: 2 }}>
            <FormControl size="small" sx={{ minWidth: 120 }}>
              <InputLabel sx={{ color: '#fff' }}>Normativa</InputLabel>
              <Select
                value={norm}
                onChange={(e) => handleNormChange(e.target.value as any)}
                sx={{
                  color: '#fff',
                  '& .MuiOutlinedInput-notchedOutline': { borderColor: '#525252' },
                  '&:hover .MuiOutlinedInput-notchedOutline': { borderColor: '#666' },
                  '& .MuiSvgIcon-root': { color: '#fff' },
                }}
              >
                <MenuItem value="IEC">IEC</MenuItem>
                <MenuItem value="NEC">NEC</MenuItem>
                <MenuItem value="Personalizada">Personalizada</MenuItem>
              </Select>
            </FormControl>

            <Button
              variant="outlined"
              startIcon={<ArrowBackIcon />}
              onClick={handleGoBack}
              sx={{ 
                borderColor: '#666',
                color: '#e0e0e0',
                '&:hover': { borderColor: '#777', backgroundColor: 'rgba(255, 255, 255, 0.05)' },
              }}
            >
              Volver
            </Button>
          </Box>
        </Box>

        {/* Barra de progreso */}
        <Box sx={{ marginTop: 3 }}>
          <Box sx={{ display: 'flex', justifyContent: 'space-between', alignItems: 'center', marginBottom: 1 }}>
            <Typography variant="body2" sx={{ color: '#b0b0b0' }}>
              Progreso del proyecto
            </Typography>
            <Typography variant="body2" sx={{ color: '#e0e0e0', fontWeight: 'bold' }}>
              {progress.completed}/{progress.total} secciones calculadas ({Math.round(progress.percentage)}%)
            </Typography>
          </Box>
          <LinearProgress 
            variant="determinate" 
            value={progress.percentage}
            sx={{
              height: 8,
              borderRadius: 4,
              backgroundColor: '#525252',
              '& .MuiLinearProgress-bar': {
                backgroundColor: progress.percentage === 100 ? '#81c784' : '#ffb74d',
                borderRadius: 4,
              },
            }}
          />
        </Box>
      </Paper>

      {/* Mensajes de estado */}
      {error && (
        <Alert 
          severity="error" 
          sx={{ marginBottom: 2, backgroundColor: '#4a3a3a', color: '#ffab91' }}
          onClose={() => setError("")}
        >
          {error}
        </Alert>
      )}

      {success && (
        <Alert 
          severity="success" 
          sx={{ marginBottom: 2, backgroundColor: '#3a4a3a', color: '#a5d6a7' }}
          onClose={() => setSuccess("")}
        >
          {success}
        </Alert>
      )}

      {/* Advertencia de cambios no guardados */}
      {hasUnsavedChanges && (
        <Alert 
          severity="warning" 
          sx={{ marginBottom: 2, backgroundColor: '#4a4a3a', color: '#ffcc80' }}
        >
          Tienes cambios sin guardar en la configuraci√≥n de normativa.
        </Alert>
      )}

      {/* Pesta√±as principales */}
      <Paper elevation={6} sx={{ 
        backgroundColor: '#3a3a3a',
        borderRadius: '16px',
        border: '1px solid #525252',
        overflow: 'hidden'
      }}>
        {/* Navegaci√≥n de pesta√±as */}
        <Box sx={{ borderBottom: 1, borderColor: '#525252' }}>
          <Tabs
            value={activeTab}
            onChange={(_, newValue) => setActiveTab(newValue)}
            variant="scrollable"
            scrollButtons="auto"
            sx={{
              '& .MuiTab-root': {
                color: '#b0b0b0',
                fontWeight: 'bold',
                textTransform: 'none',
                fontSize: '14px',
                minHeight: '72px',
                '&.Mui-selected': {
                  color: '#ffb74d',
                },
                '&.Mui-disabled': {
                  color: '#666',
                  opacity: 0.5,
                },
              },
              '& .MuiTabs-indicator': {
                backgroundColor: '#ffb74d',
                height: 3,
              },
            }}
          >
            {tabs.map((tab, index) => {
              const calculation = index < tabs.length - 2 ? calculations[getCalculationKey(tab.circuitType)] : null;
              const status = calculation?.status;
              
              return (
                <Tab
                  key={index}
                  disabled={!tab.enabled}
                  icon={tab.icon}
                  label={
                    <Box sx={{ textAlign: 'center' }}>
                      <Typography variant="body2" sx={{ fontWeight: 'bold' }}>
                        {tab.label}
                        {!tab.enabled && " (Pr√≥ximamente)"}
                      </Typography>
                      <Typography variant="caption" sx={{ color: '#888', display: 'block' }}>
                        {tab.description}
                      </Typography>
                      {status && (
                        <Chip
                          size="small"
                          label={status === 'success' ? 'Calculado' : status === 'error' ? 'Error' : 'Calculando'}
                          sx={{ 
                            marginTop: 0.5,
                            height: 16,
                            fontSize: '10px',
                            backgroundColor: status === 'success' ? '#3a4a3a' : status === 'error' ? '#4a3a3a' : '#4a4a3a',
                            color: status === 'success' ? '#a5d6a7' : status === 'error' ? '#ffab91' : '#ffcc80',
                          }}
                        />
                      )}
                    </Box>
                  }
                  iconPosition="top"
                />
              );
            })}
          </Tabs>
        </Box>

        {/* Contenido de las pesta√±as */}
        {tabs.map((tab, index) => (
          <TabPanel key={index} value={activeTab} index={index}>
            {index === 0 ? (
              // Pesta√±a de strings (habilitada)
              <StringsTabContent
                calculation={calculations.strings}
                onCalculate={executeStringCalculation}
                onClear={clearResults}
                isLoading={isLoading}
                norm={norm}
                isAnalysisExpanded={isAnalysisExpanded}
                setIsAnalysisExpanded={setIsAnalysisExpanded}
              />
            ) : index === 4 ? (
              // Pesta√±a de configuraci√≥n
              <NormativeConfigTabContent
                normativeConfig={normativeConfig}
                setNormativeConfig={setNormativeConfig}
                norm={norm}
                isLoading={normativeLoading}
                onSave={saveNormativeConfig}
                onReset={resetNormativeConfig}
                hasUnsavedChanges={hasUnsavedChanges}
                setHasUnsavedChanges={setHasUnsavedChanges}
              />
            ) : index === 5 ? (
              // Pesta√±a de resumen
              <SummaryTabContent
                calculations={calculations}
                projectName={currentProjectName}
                progress={progress}
              />
            ) : index < tabs.length - 2 ? (
              // Pesta√±as de otros c√°lculos (deshabilitadas)
              <DisabledTabContent tab={tab} />
            ) : null}
          </TabPanel>
        ))}
      </Paper>
    </Box>
  );
}

// Componente para configuraci√≥n de normativas
interface NormativeConfigTabContentProps {
  normativeConfig: NormativeConfig | null;
  setNormativeConfig: React.Dispatch<React.SetStateAction<NormativeConfig | null>>;
  norm: string;
  isLoading: boolean;
  onSave: () => void;
  onReset: () => void;
  hasUnsavedChanges: boolean;
  setHasUnsavedChanges: (value: boolean) => void;
}

function NormativeConfigTabContent({ 
  normativeConfig, 
  setNormativeConfig, 
  norm, 
  isLoading, 
  onSave, 
  onReset,
  hasUnsavedChanges,
  setHasUnsavedChanges
}: NormativeConfigTabContentProps) {
  const handleParameterChange = (sectionKey: string, paramKey: string, newValue: any) => {
    if (!normativeConfig) return;

    setNormativeConfig({
      ...normativeConfig,
      editable_sections: {
        ...normativeConfig.editable_sections,
        [sectionKey]: {
          ...normativeConfig.editable_sections[sectionKey],
          parameters: {
            ...normativeConfig.editable_sections[sectionKey].parameters,
            [paramKey]: {
